<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.odn.selectorimage">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name="com.odn.selectorimage.view.ImageSelectorActivity"
            android:theme="@style/AppTheme.Base" />
        <activity
            android:name="com.odn.selectorimage.view.ImagePreviewActivity"
            android:theme="@style/AppTheme.Base" />
        <activity
            android:name="com.odn.selectorimage.view.ImageCropActivity"
            android:theme="@style/AppTheme.Base" />
    </application>


</manifest>
